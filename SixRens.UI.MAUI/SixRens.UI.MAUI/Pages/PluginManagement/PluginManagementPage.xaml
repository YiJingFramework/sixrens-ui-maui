<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:coreppm="clr-namespace:SixRens.Core.插件管理.插件包管理;assembly=SixRens.Core"
             x:Class="SixRens.UI.MAUI.Pages.PluginManagement.PluginManagementPage"
             Title="PluginManagementPage" >
    <ScrollView>
        <VerticalStackLayout>
            <Label Text="插件包" />
            <Button Text="配置插件包" Clicked="OnManagePackagesClicked" />

            <Label Text="暂时" />
            <Button Text="清除全部插件包" Clicked="ClearPackages"/>
            <Button Text="清除全部预设"  Clicked="ClearPresets"/>
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>